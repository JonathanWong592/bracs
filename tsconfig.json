{
    "compilerOptions": {
      "target": "ES2017",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "ESNext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "jsx": "react-jsx",
      "noFallthroughCasesInSwitch": true,
      "typeRoots": [
        // "./src/core/types",
      ]
    },
    "include": [
      "src"
    ],
    "typeAcquisition": {
      "include": [
        // "./src/core/index.d.ts",
        // "./src/core/helpers.d.ts",
        // "./src/core/swagger.d.ts"
      ]
    }
  }
  